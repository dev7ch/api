{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "node": "8.10",
        "browsers": ["> 1%", "last 2 versions", "not ie <= 11"]
      }
    }],
    "stage-2",

    "es2015"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    ["@babel/plugin-proposal-class-properties"]
  ]
}